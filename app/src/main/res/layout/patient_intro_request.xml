<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Patient_intro_Request">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="15dp"
        android:forceHasOverlappingRendering="true"
        app:srcCompat="@drawable/left_arrow" />

    <Button
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="50dp"
        android:text="Request"
        android:textAlignment="textStart"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/new_request_icon"
        android:layout_width="173dp"
        android:layout_height="198dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="130dp"
        app:srcCompat="@drawable/new_request_icon" />

    <ImageView
        android:id="@+id/track_request_icon"
        android:layout_width="245dp"
        android:layout_height="251dp"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="90dp"
        app:srcCompat="@drawable/track_request_icon" />

    <ImageView
        android:id="@+id/history_icon"
        android:layout_width="229dp"
        android:layout_height="271dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="370dp"
        app:srcCompat="@drawable/history_icon" />

</RelativeLayout>