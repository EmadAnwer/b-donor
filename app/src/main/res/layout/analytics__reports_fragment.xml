<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/analytic_reports"
    tools:context=".Analytics_Reports_fragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/back_from_help_support"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:forceHasOverlappingRendering="true"
            android:src="@drawable/left_arrow" />

        <Button
            android:layout_width="370dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:text="Analytice Reports"
            android:textAlignment="textStart"
            android:textStyle="bold" />

    </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="20dp">

            <Button
                android:id="@+id/A_negative_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:checkable="true"
                android:clickable="true"
                android:enabled="true"
                android:text="A -"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909" />

            <Button
                android:id="@+id/A_positive_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:enabled="true"
                android:text="A+"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909"
                android:layout_marginLeft="50dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="10dp">

            <Button
                android:id="@+id/B_negative_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:enabled="true"
                android:text="B -"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909"
                android:layout_marginRight="30dp"/>


            <Button
                android:id="@+id/B_positive_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:checkable="true"
                android:clickable="true"
                android:enabled="true"
                android:text="B +"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909"
                android:layout_marginLeft="18dp"/>

        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="10dp">

            <Button
                android:id="@+id/O_negative_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:enabled="true"
                android:text="O -"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909"
                android:layout_marginRight="30dp"/>


            <Button
                android:id="@+id/O_positive_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:backgroundTint="#FFD8D7D7"
                android:checkable="true"
                android:clickable="true"
                android:enabled="true"
                android:text="O +"
                android:textSize="30dp"
                android:textAlignment="center"
                android:textColor="#090909"
                android:layout_marginLeft="18dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="10dp">


            <Button
                android:id="@+id/AB_negative_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginRight="30dp"
                android:backgroundTint="#FFD8D7D7"
                android:enabled="true"
                android:text="AB -"
                android:textAlignment="center"
                android:textColor="#090909"
                android:textSize="30dp" />

            <Button
                android:id="@+id/AB_positive_report"
                android:layout_width="140dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="18dp"
                android:backgroundTint="#FFD8D7D7"
                android:checkable="true"
                android:clickable="true"
                android:enabled="true"
                android:text="AB +"
                android:textAlignment="center"
                android:textColor="#090909"
                android:textSize="30dp" />

        </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</FrameLayout>